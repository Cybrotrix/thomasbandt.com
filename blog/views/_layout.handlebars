    <meta name="google-site-verification" content="M3wbVWxqz4moQM2YrDGSjooMol04iox4e-VktU1gN6E">
    {{{sections.pageStyles}}}
</head>
<body>
    <div class="container blog-container">
        <header>
            <img src="/blog/client/images/profile-picture@2x.png"
                 srcset="/blog/client/images/profile-picture.png 1x,
                         /blog/client/images/profile-picture@2x.png 2x,
                         /blog/client/images/profile-picture@3x.png 3x"
                 {{#if hideProfilePicture}}class="hidden-on-desktop"{{/if}}
                 alt="Thomas Bandt">

            <nav class="header-content">
                <div class="title">
                    <strong>Thomas Bandt <span class="hide-on-mobile">&dash;</span></strong>
                    <span class="subtitle">Developer &amp; Entrepreneur of Passion.</span>
                </div>
                <br>
                <a href="{{routes.blog.home}}" {{{activeMenuItem routes.blog.home activeRoute}}}>Home</a>
                <a href="{{routes.blog.about}}" {{{activeMenuItem routes.blog.about activeRoute}}}>About</a>
                <a href="{{routes.blog.contact}}" {{{activeMenuItem routes.blog.contact activeRoute}}}>Contact</a>
                <a href="{{routes.blog.archives}}" {{{activeMenuItem routes.blog.archives activeRoute}}}>Archives</a>
                <a href="https://feeds.feedburner.com/ThomasGoesNet" target="_blank"><span class="hide-on-mobile">Feed </span><i class="fa fa-rss"></i></a>
            </nav>
        </header>

        <div class="content">
        {{{body}}}
        </div>

        <footer class="page-footer">
            <img src="/blog/client/images/spacer@2x.png"
                 srcset="/blog/client/images/spacer.png 1x,
                         /blog/client/images/spacer@2x.png 2x,
                         /blog/client/images/spacer@3x.png 3x"
                 alt=""><br>
            &copy; 1999 &dash; {{currentYear}}<br><a href="{{routes.blog.about}}">Thomas Bandt, Munich</a>
        </footer>
    </div>
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-58418408-1', 'auto');
        ga('send', 'pageview');
        ga('set', 'anonymizeIp', true);
    </script>
</body>
</html>
