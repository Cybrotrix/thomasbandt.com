    <link rel="apple-touch-icon" sizes="57x57" href="/blog/client/images/favicons/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/blog/client/images/favicons/apple-touch-icon-114x114.png">
